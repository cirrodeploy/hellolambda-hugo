{
    "dev": {
        "app_function": "hellolambda_api.api.app",
        "profile_name": "hellolambda", // set a profile you have defined in your .aws/credentials file
        "keep_warm": false,
        "cors": true,
        "manage_roles": true,

        // These cause the following error: botocore.errorfactory.BadRequestException: An error occurred (BadRequestException) when calling the UpdateStage operation: CloudWatch Logs role ARN must be set in account settings to enable logging
        "cloudwatch_log_level": "INFO",
        "cloudwatch_data_trace": true
    }
}